# Quaero Environment Variables
# Copy this file to .env and fill in your values
# This file is loaded into the KV store at application startup

# Google Gemini API Key (required for AI features)
# Get your key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=

# GitHub Personal Access Token (required for GitHub connectors)
# Needs 'repo' and 'workflow' scopes
GITHUB_TOKEN=

# SMTP Configuration (for email notifications)
SMTP_HOST=smtp.gmail.com
SMTP_USERNAME=
SMTP_PASSWORD=
SMTP_FROM=

# EODHD API (required for ASX stock data - fundamentals, prices, financials)
# Get your key from: https://eodhd.com/
EODHD_API_KEY=

# Navexa API (for portfolio tracking and performance)
# Get your key from your Navexa account settings
navexa_api_key=

# Optional: Override default ports
# PORT=8080
# LOG_LEVEL=info
